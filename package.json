{
  "name": "pod_app",
  "version": "1.0.0",
  "description": "Online Learning App.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arielcc88/POD_APP.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arielcc88/POD_APP/issues"
  },
  "homepage": "https://github.com/arielcc88/POD_APP#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "custom-env": "^2.0.1",
    "express": "^4.16.3",
    "foundation-sites": "^6.6.3",
    "http": "0.0.1-security",
    "if-env": "^1.0.4",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.6.4",
    "nodemailer": "^6.4.11",
    "sequelize": "^4.41.2",
    "sequelize-fixtures": "^1.2.0",
    "what-input": "^4.1.0",
    "if-env": "1.0.4"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^2.0.4"
  }
}
